    "webpack-merge": "4.1.2",
    "webpack-stream": "4.0.3"
  },
  "keywords": [
    "hots",
    "api"
  ],
  "author": "seweren",
  "repository": "https://github.com/seweren/ahotspage.git",
  "license": "MIT"
}
