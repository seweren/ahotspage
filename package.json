{
  "name": "ahotspage",
  "version": "0.1.1",
  "description": "A Heroes of the Storm page",
  "main": "gulpfile.ts",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "install": "gulp",
    "start": "node ./server/server.js",
    "test": "jest --config=jest.json ./tests/unittests.ts"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tests.tsconfig.json"
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "*/**/unittests.ts"
    ],
    "collectCoverage": true,
    "mapCoverage": true,
    "coverageReporters": [
      "text",
      "html"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "dependencies": {
    "@types/clone": "0.1.30",
    "@types/express": "4.11.1",
    "@types/gulp": "4.0.5",
    "@types/morgan": "1.7.35",
    "@types/node": "10.0.4",
    "@types/react": "16.3.13",
    "@types/react-dom": "16.0.5",
    "@types/react-redux": "5.0.19",
    "@types/react-router-dom": "4.2.6",
    "clone": "2.1.1",
    "express": "4.16.3",
    "gulp": "4.0.0",
    "material-ui": "1.0.0-beta.33",
    "morgan": "1.9.0",
    "node": "10.0.0",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "redux": "4.0.0",
    "redux-persist": "5.9.1",
    "ts-node": "6.0.2",
    "typescript": "2.8.3"
  },
  "devDependencies": {
    "@types/gulp-mocha": "0.0.32",
    "@types/jest": "22.2.3",
    "@types/mocha": "5.2.0",
    "@types/react-hot-loader": "4.1.0",
    "@types/source-map": "^0.5.2",
    "@types/webdriverio": "4.10.1",
    "@types/webpack": "4.1.4",
    "@types/webpack-dev-middleware": "2.0.1",
    "@types/webpack-dev-server": "2.9.4",
    "@types/webpack-env": "1.13.6",
    "@types/webpack-hot-middleware": "2.16.3",
    "@types/webpack-merge": "4.1.3",
    "@types/webpack-stream": "3.2.10",
    "awesome-typescript-loader": "5.0.0",
    "css-loader": "0.28.11",
    "gulp-mocha": "5.0.0",
    "gulp-tslint": "8.1.3",
    "gulp-webdriver": "2.0.3",
    "jest": "22.4.3",
    "mocha": "5.1.1",
    "react-hot-loader": "4.1.2",
    "selenium-standalone": "6.14.0",
    "source-map": "^0.7.2",
    "style-loader": "0.21.0",
    "ts-jest": "22.4.4",
    "tslint": "5.10.0",
    "typescript-fsa": "^3.0.0-beta-1",
    "url-loader": "1.0.1",
    "util-deprecate": "1.0.2",
    "wdio-dot-reporter": "0.0.9",
    "wdio-mocha-framework": "0.5.13",
    "wdio-selenium-standalone-service": "0.0.10",
    "webdriverio": "4.12.0",
    "webpack": "4.7.0",
