{
  "version": "2.0.0",
  "type": "shell",
  "tasks": [
    {
      "label": "compile-server",
      "command": "gulp compile-server",
    },
    {
      "label": "run-tests",
      "command": "gulp run-tests",
      "group": {
        "kind": "test",
        "isDefault": true
      }
    },
    {
      "label": "default",
      "command": "gulp",
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$tsc",
        "$gulp-tsc"
      ]
    },
    {
      "label": "tslint-all",
      "command": "gulp tslint-all",
      "problemMatcher": [
        "$tslint5",
      ]
    }
  ]
}
